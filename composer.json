{
  "name": "garrettw/wiscord",
  "description": "An unofficial API to interact with the voice and text chat service Discord.",
  "license": "MIT",
  "authors": [
    {
      "name": "Garrett Whitehorn",
      "role": "Lead Developer",
      "homepage": "http://garrettw.net/"
    },
    {
      "name": "David Cole",
      "role": "Original Developer",
      "email": "david@team-reflex.com"
    },
    {
      "name": "Aaron Scherer",
      "role": "Original Contributor",
      "email": "aequasi@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "psr/log": "^1.0.1",
    "monolog/monolog": "^1.21",
    "symfony/options-resolver": "^3.3",
    "react/event-loop": "^0.4",
    "ratchet/pawl": "^0.2.2",
    "clue/buzz-react": "^1.2"
  },
  "require-dev": {
    "symfony/var-dumper": "^3.3",
    "phpspec/phpspec": "^3.0"
  },
  "autoload": {
    "files": ["src/functions.php"],
    "psr-4": {
      "Wiscord\\": "src/"
    }
  },
  "suggest": {
    "cache/apc-adapter": "Required to use the APC cache driver",
    "cache/memcache-adapter": "Required to use the Memcache cache driver",
    "cache/memcached-adapter": "Required to use the Memcached cache driver",
    "cache/redis-adapter": "Required to use the Redis cache driver",
    "ext-libevent": "Improves event loop peformance (preferred)",
    "ext-libev": "Improves event loop peformance",
    "ext-event": "Improves event loop peformance"
  }
}
